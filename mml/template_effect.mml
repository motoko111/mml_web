// effect

// Pulse
@[pulse]
t120 l8 o4 q90 v50 @0 @e[0,0.1,0.6,0.2] @m0

// コマンドのリセット
@c[reset]

// ピッチ 変化する高さ(20で半音下げる -20で半音上げる) , 遅延時間, 変化時間 
@c[p,-240,0.1,0.1]
cdef
@c[reset]

// ビブラート 変化する高さ, 深度, 遅延時間
@c[v,10,0.1,0.5]
c^^^^^
@c[reset]

// アルペジオ 1音変化の長さ, 変化する音の高さ(半音単位)
@c[c,64,0,4,7]
c^^^
@c[reset]
ceg
@c[c,8,0,4,7]
c^^
@c[reset]

;


